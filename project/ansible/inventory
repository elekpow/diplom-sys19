bastion-elvm:
  hosts:
    51.250.14.150:  
websrv-elvm:
  hosts:
    172.16.121.10:
    172.16.122.10:  
  vars:
    ansible_python_interpreter: '/usr/bin/python3'
    ansible_ssh_user: 'igor'
    ansible_ssh_common_args: '-o StrictHostKeyChecking=no -o ProxyCommand="ssh -W %h:%p -q bastion@51.250.14.150"'    
