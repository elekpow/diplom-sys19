bastion-elvm:
  hosts:
    #bastionhost1:  
websrv-elvm:
  hosts:
    #webhost1:
    #webhost2:  
  vars:
    ansible_python_interpreter: '/usr/bin/python3'
    ansible_ssh_user: 'igor'
    ansible_ssh_common_args: '-o StrictHostKeyChecking=no -o ProxyCommand="ssh -W %h:%p -q bastion@#bastionhost1"'    
